{
  "name": "kapture",
  "version": "0.0.1",
  "description": "To encourage interaction with nature by capturing images/descriptions of native birds",
  "main": "server/index.js",
  "scripts": {
    "dev:client": "webpack --watch --config client/webpack.config.js",
    "dev:server": "nodemon --watch server",
    "client": "webpack --config client/webpack.config.js",
    "dev": "run-p dev:client dev:server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "webpack:server": "webpack-dev-server --hot --inline --content-base server/public --config client/webpack.config.js",
    "test": "jest --watchAll --verbose",
    "server": "node server",
    "start": "node server",
    "lint": "eslint --ext .js,.jsx client server",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:reset": "npm run knex migrate:rollback && npm run db:migrate",
    "build": "run-s client db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/horoeka-2020/kapture"
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "slick",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/horoeka-2020/kapture/issues"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.3",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/user-event": "^12.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "7.13.0",
    "eslint-config-eda": "^0.1.4",
    "jest": "^26.1.0",
    "msw": "^0.19.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "supertest": "^4.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "sqlite3": "^4.2.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.21.12",
    "pg": "^8.5.1",
    "react-router-dom": "^5.2.0",
    "superagent": "^5.3.1"
  },
  "homepage": "https://github.com/horoeka-2020/kapture"
}
